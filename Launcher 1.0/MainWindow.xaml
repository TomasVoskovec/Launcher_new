<Window x:Class="Launcher_1._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher_1._0"
        mc:Ignorable="d"
        Title="Launcher 1.0"
        Height="450" Width="800">
    <Grid>
        <WrapPanel x:Name="iconPanel" Margin="25" Background="#FF0C0C0C" Orientation="Horizontal"/>
        <Button Width="200" Height="40" VerticalAlignment="Bottom" Click="selectPath_click">Select .csproj file</Button>
    </Grid>
</Window>
